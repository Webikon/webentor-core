{
  "$schema": "../../../schemas/webentor-block.json",
  "apiVersion": 3,
  "name": "webentor/e-tab-container",
  "title": "Single Tab",
  "description": "Tab used in Tabs block.",
  "category": "webentor-elements",
  "icon": "table-row-after",
  "keywords": [
    "tabs",
    "container",
    "elements"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "hideTitle": {
      "type": "boolean",
      "default": false
    },
    "template": {
      "type": "array",
      "default": null
    },
    "spacing": {
      "type": "object",
      "default": {
        "padding-top": {
          "value": {
            "basic": "pt-5",
            "md": "pt-8"
          }
        },
        "padding-bottom": {
          "value": {
            "basic": "pb-4"
          }
        }
      }
    }
  },
  "supports": {
    "webentor": {
      "display": true
    }
  }
}
