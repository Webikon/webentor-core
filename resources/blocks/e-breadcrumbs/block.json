{
  "$schema": "../../../schemas/webentor-block.json",
  "name": "webentor/e-breadcrumbs",
  "apiVersion": 3,
  "title": "Breadcrumbs",
  "description": "Block showing breadcrumbs.",
  "category": "webentor-elements",
  "icon": "open-folder",
  "keywords": [
    "breadcrumbs"
  ],
  "attributes": {
    "anchor": {
      "type": "string",
      "default": ""
    },
    "spacing": {
      "type": "object",
      "default": {
        "padding-top": {
          "value": {
            "basic": "pt-3.5",
            "lg": "pt-4"
          }
        },
        "padding-bottom": {
          "value": {
            "basic": "pb-3.5",
            "lg": "pb-4"
          }
        }
      }
    }
  },
  "supports": {
    "anchor": true,
    "webentor": {
      "spacing": true
    }
  }
}
